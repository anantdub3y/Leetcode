class Solution {
public:
    bool isCovered(vector<vector<int>>& ranges, int left, int right) {
        vector<int> diff(52, 0); 

        for (auto &r : ranges) {
            diff[r[0]]++;
            if (r[1] + 1 <= 50)
                diff[r[1] + 1]--;
        }

        int curr = 0;
        for (int i = 1; i <= 50; i++) {
            curr += diff[i];
            if (i >= left && i <= right && curr <= 0) {
                return false;
            }
        }
        return true;
    }
};
